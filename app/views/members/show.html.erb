<% provide(:title, 'Course members') %>
<h1><%= @course.name %></h1>
<%= will_paginate %>
<ul class="users">
  <% @members.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
      | <%= user.suppervisor? ? 'Suppervisor' : 'Member' %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
