<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>        
      </h1>
      Member
    </section>
    <h1>
      List subjects
    </h1>
    <ul class="users">
      <%= render @subjects %>
    </ul>
  </aside>
  <div class="col-md-8">
  </div>
</div>
